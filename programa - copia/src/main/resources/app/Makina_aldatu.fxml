                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem onAction="#piezakMenu" mnemonicParsing="false" text="Piezak" />
               </items>
            </MenuButton>
            <ImageView fitHeight="47.0" fitWidth="125.0" layoutX="543.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@ALFAJOR/ALFAJOR/Mahaiganeko%20aplikazioa%20(Scene%20Builder)/Argazkiak/images.png" />
               </image>
            </ImageView>
            <Label alignment="CENTER" layoutX="164.0" layoutY="69.0" prefHeight="18.0" prefWidth="310.0" text="Makinaren informazioa aldatu">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Label layoutX="66.0" layoutY="122.0" prefHeight="22.0" prefWidth="48.0" text="ID">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="Id_makina" layoutX="122.0" layoutY="119.0" prefHeight="27.0" prefWidth="383.0" />
            <Label layoutX="66.0" layoutY="147.0" prefHeight="22.0" prefWidth="48.0" text="Izena">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="Izena" layoutX="122.0" layoutY="145.0" prefHeight="27.0" prefWidth="383.0" />
            <Label layoutX="66.0" layoutY="181.0" prefHeight="18.0" prefWidth="96.0" text="Deskribapena">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="Deskribapena" layoutX="178.0" layoutY="179.0" prefHeight="27.0" prefWidth="327.0" />
            <Label layoutX="66.0" layoutY="222.0" prefHeight="18.0" prefWidth="96.0" text="Potentzia(KW)">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="Potentzia" layoutX="178.0" layoutY="219.0" prefHeight="27.0" prefWidth="327.0" />
            <Label layoutX="66.0" layoutY="260.0" prefHeight="22.0" prefWidth="115.0" text="Instalazio Data">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="Instalazio_data" layoutX="178.0" layoutY="258.0" prefHeight="27.0" prefWidth="327.0" promptText="YYYY-MM-DD"/>
            <Button onAction="#makinaAldatu" layoutX="430.0" layoutY="330.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="70.0" text="ALDATU" />
            <Button onAction="#Bueltatu" layoutX="511.0" layoutY="330.0" mnemonicParsing="false" text="BUELTATU" />
         </children>
      </AnchorPane>
   </children>
</VBox>
