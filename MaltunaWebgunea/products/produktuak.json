{   "ERABILTZAILEA": 
    { "columns": 
        { "Id_erabiltzailea": "VARCHAR(4) PRIMARY KEY", 
        "Izena": "VARCHAR(15)", 
        "Abizena1": "VARCHAR(20)", 
        "NAN": "VARCHAR(9)", 
        "Helbidea": "VARCHAR(40)", 
        "PostaKodea": "INT", 
        "Email": "VARCHAR(40)", 
        "J_data": "DATE", 
        "Alta_data": "DATE" 
        } 
    }, 
    "PIEZA_MOTA": 
    { "columns": 
        { "Id_pieza_mota": "VARCHAR(4) PRIMARY KEY", 
        "Izena": "VARCHAR(15)" 
        } 
    }, 
    "PIEZA": 
    { "columns": 
        { "Id_pieza": "VARCHAR(4) PRIMARY KEY", 
        "Id_pieza_mota": "VARCHAR(4) FOREIGN KEY", 
        "Izena": "VARCHAR(15)", 
        "Deskribapena": "VARCHAR(150)", 
        "Pisua": "VARCHAR(15)", 
