        "Stock": "INT CHECK (Stock >= 0)"
        }, 
        "foreign_keys": { "Id_pieza_mota": 
        { "references": "PIEZA_MOTA(Id_pieza_mota)", "on_update": "CASCADE", "on_delete": "CASCADE" } 
        } 
    }, 
    "MAKINA": 
    { "columns": 
        { "Id_makina": "VARCHAR(4) PRIMARY KEY", 
        "Izena": "VARCHAR(30)", 
        "Instalazio_data": "VARCHAR(20)", 
        "Deskribapena": "VARCHAR(150)", 
        "Potentzia": "INT" 
        } 
    }, 
    "ERABILTZAILEA_MAKINA":
    { "columns":
        { "Id_erabiltzailea": "VARCHAR(4)", 
        "Id_makina": "VARCHAR(4)", 
        "Hasiera_data": "DATE", 
        "Amaiera_data": "DATE" }, 
        "primary_key": [ "Id_erabiltzailea", "Id_makina", "Hasiera_data" ], 
        "foreign_keys": { "Id_erabiltzailea": { "references": "ERABILTZAILEA(Id_erabiltzailea)", "on_update": "CASCADE", "on_delete": "CASCADE" }, 
        "Id_makina": { "references": "MAKINA(Id_makina)", "on_update": "CASCADE", "on_delete": "CASCADE" } 
        } 
    } 
}